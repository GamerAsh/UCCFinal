<!doctype html>
<html>
<head>
  <title> <%= title %> </title>
  <%= csrf_meta_tag %>
  <%= render 'layouts/stylesheets' %>
  <%= javascript_include_tag :defaults %>

</head>
<body>
<div class="main">
  <div class="content">
    <%= render 'layouts/navigation' %>
    <section class="round">
      <% flash.each do |key, value| %>
          <div class="flash <%= key %>"><%= value %>
          </div>
      <% end %>
      <%= yield %>
    </section>

    <%= render 'layouts/botnavigation' %>
    <%= debug(params) if Rails.env.development? %>
  </div>
</div>
</body>
</html>
